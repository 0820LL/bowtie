IF TYPE eq ps   set terminal postscript landscape color
IF TYPE eq eps  set terminal postscript eps color
IF TYPE eq jpg  set terminal jpeg
IF TYPE eq png  set terminal png
IF TYPE eq pbm  set terminal pbm
IF TYPE ne xwin set output "%OUTFILE%"
set xlabel "%XLABEL%" font "%FONT%,%FONTSIZE%"
set ylabel "%YLABEL%" font "%FONT%,%FONTSIZE%"
set logscale x
set multiplot
#set autoscale
set data style lines
set border 3
set xtics border nomirror ("1/16\n(180 Mbases)" 179451189.0, "1/8\n(359 Mbases)" 358902379.0, "1/4\n(718 Mbases)" 717804758.0, "1/2\n(1.44 Gbases)" 1435609516.0, "Whole\n(2.87 Gbases)" 2871219032.0)
#set xtics border nomirror (179451189.0, 358902379.0, 717804758.0, 1435609516.0, 2871219032.0)
set xrange [149542657.5:3445462838.4]
set mytics 5
set ytics border mirror 150, 50, 600
set y2tics border mirror 150, 50, 600
set yrange [150:600]
set y2range [150:600]
set origin 0.0,0.0
set title "%TITLE%" font "%TFONT%,%TFONTSIZE%"
